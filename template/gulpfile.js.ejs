//GULP

var gulp  = require('gulp');
var shell = require('gulp-shell');
 
 

gulp.task('build', function()
{
        .pipe(shell([
            "npm run generate-gitbook"
        ]))        
       
});


gulp.task('default', function(){
    gulp.watch(['txt/*.md', 'scripts/**',  'book.json'], ['gitbook']); 
});